  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Program (PREINIT)",
      "skipFiles": ["<node_internals>/**"],
      "env": {
        "PREINIT": "1",
        "PORT": "8000"
      },
      "program": "${workspaceFolder}/bin/www",
      "outFiles": ["${workspaceFolder}/**/*.js"]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/bin/www",
      "outFiles": ["${workspaceFolder}/**/*.js"],
      "env": {
        "PORT": "8000"
      }
    }
  ]
}
